{
  "expo": {
    "name": "Quick Loans",
    "slug": "qloansapp",
    "version": "1.0.5",
    "orientation": "portrait",
	"owner": "walter10h",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#abacab",
          "mode": "production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.quickloans.qloansapp",
      "buildNumber": "1.0.5"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.quickloans.qloansapp",
      "versionCode": 8,
      "permissions": [],
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json"
    }
  }
}
