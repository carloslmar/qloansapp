{
  "expo": {
    "name": "Quick Loans",
    "slug": "qloansapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
	"userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
	  "bundleIdentifier": "com.quickloans.qloansapp",
	  "buildNumber": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.quickloans.qloansapp",
	  "versionCode": 1,
	  "useNextNotificationsApi": true
    }
  }
}
